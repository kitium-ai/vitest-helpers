{
  "name": "@kitiumai/vitest-helpers",
  "version": "2.0.1",
  "description": "Vitest test helpers with framework-specific utilities - setup, browser testing, benchmarks, and migration tools",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitium-ai/vitest-helpers.git"
  },
  "homepage": "https://github.com/kitium-ai/vitest-helpers#readme",
  "bugs": {
    "url": "https://github.com/kitium-ai/vitest-helpers/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./setup": {
      "types": "./dist/setup/index.d.ts",
      "default": "./dist/setup/index.js"
    },
    "./setup/presets": {
      "types": "./dist/setup/presets.d.ts",
      "default": "./dist/setup/presets.js"
    },
    "./setup/workspace": {
      "types": "./dist/setup/workspace.d.ts",
      "default": "./dist/setup/workspace.js"
    },
    "./compatibility": {
      "types": "./dist/compatibility/index.d.ts",
      "default": "./dist/compatibility/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "default": "./dist/browser/index.js"
    },
    "./benchmarks": {
      "types": "./dist/benchmarks/index.d.ts",
      "default": "./dist/benchmarks/index.js"
    },
    "./reporters": {
      "types": "./dist/reporters/index.d.ts",
      "default": "./dist/reporters/index.js"
    },
    "./tracing": {
      "types": "./dist/tracing/index.d.ts",
      "default": "./dist/tracing/index.js"
    },
    "./migration": {
      "types": "./dist/migration/index.d.ts",
      "default": "./dist/migration/index.js"
    },
    "./examples": {
      "types": "./dist/examples/index.d.ts",
      "default": "./dist/examples/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "default": "./dist/config/index.js"
    },
    "./lint": {
      "types": "./dist/lint/index.d.ts",
      "default": "./dist/lint/index.js"
    },
    "./tooling": {
      "types": "./dist/tooling/index.d.ts",
      "default": "./dist/tooling/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "default": "./dist/plugins/index.js"
    },
    "./ci": {
      "types": "./dist/ci/index.d.ts",
      "default": "./dist/ci/index.js"
    },
    "./monitoring": {
      "types": "./dist/monitoring/index.d.ts",
      "default": "./dist/monitoring/index.js"
    },
    "./collaboration": {
      "types": "./dist/collaboration/index.d.ts",
      "default": "./dist/collaboration/index.js"
    },
    "./performance": {
      "types": "./dist/performance/index.d.ts",
      "default": "./dist/performance/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "default": "./dist/cli/index.js"
    },
    "./visual": {
      "types": "./dist/visual/index.d.ts",
      "default": "./dist/visual/index.js"
    },
    "./accessibility": {
      "types": "./dist/accessibility/index.d.ts",
      "default": "./dist/accessibility/index.js"
    },
    "./security": {
      "types": "./dist/security/index.d.ts",
      "default": "./dist/security/index.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "!dist/**/__tests__/**",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "types.d.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "vitest",
    "vitest-helpers",
    "vitest-utilities",
    "vitest-testing",
    "testing",
    "test-utilities",
    "test-helpers",
    "unit-testing",
    "integration-testing",
    "e2e-testing",
    "browser-testing",
    "test-framework",
    "vitest-setup",
    "vitest-config",
    "vitest-presets",
    "vitest-workspace",
    "vitest-browser",
    "vitest-benchmarks",
    "vitest-reporters",
    "vitest-tracing",
    "vitest-migration",
    "jest-to-vitest",
    "test-migration",
    "performance-testing",
    "benchmark",
    "benchmarking",
    "custom-reporters",
    "test-reporters",
    "distributed-tracing",
    "opentelemetry",
    "test-fixtures",
    "test-mocks",
    "mock-data",
    "test-data",
    "factory-pattern",
    "builder-pattern",
    "http-mocking",
    "api-testing",
    "async-testing",
    "typescript",
    "typescript-testing",
    "enterprise",
    "production-ready",
    "tree-shakable",
    "esm",
    "vite",
    "kitium",
    "kitiumai"
  ],
  "scripts": {
    "build": "tsc -b",
    "build:clean": "npm run clean && npm run build",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc -b --noEmit",
    "type-check": "tsc -b --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vit est --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "changeset": "ensure-changeset && changeset",
    "version": "changeset version",
    "publish:set-token": "set-npm-token",
    "publish:setup": "set-npm-token",
    "publish:check": "npm whoami --registry https://registry.npmjs.org || npm login --registry https://registry.npmjs.org",
    "publish:login": "npm login --registry https://registry.npmjs.org",
    "publish:package": "pnpm run publish:check && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org",
    "publish:package:token": "pnpm run publish:setup && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org",
    "publish:package:otp": "pnpm run publish:check && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org --otp",
    "publish:dry-run": "pnpm publish --access public --no-git-checks --dry-run --registry https://registry.npmjs.org",
    "publish:dry-run:token": "pnpm run publish:setup && pnpm publish --access public --no-git-checks --dry-run --registry https://registry.npmjs.org",
    "prepublishOnly": "npm run verify",
    "verify": "npm run lint && npm run build",
    "release": "pnpm version && pnpm install --lockfile-only"
  },
  "dependencies": {
    "@kitiumai/logger": "^3.0.1",
    "@kitiumai/test-core": "^3.0.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0"
  },
  "peerDependencies": {
    "vitest": "^2.0.0",
    "typescript": "^5.6.0"
  },
  "peerDependenciesMeta": {
    "@vitest/ui": {
      "optional": true
    },
    "@vitest/browser": {
      "optional": true
    }
  },
  "devDependencies": {
    "@kitiumai/config": "^3.1.4",
    "@kitiumai/lint": "^3.0.0",
    "@kitiumai/scripts": "^2.0.0",
    "@changesets/cli": "^2.27.1",
    "@types/node": "^24.10.1",
    "@types/pixelmatch": "^5.2.4",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.6.0",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "overrides": {
      "lodash.get": "npm:lodash@^4.17.21"
    }
  }
}
